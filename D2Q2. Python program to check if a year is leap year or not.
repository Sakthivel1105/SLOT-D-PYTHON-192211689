n=input("Enter a year: ")
if n.isdigit() and int(n):
  n=int(n)
  if (n < 0):
    print("Enter a valid year!")
  elif(n%400==0)and(n%100==0):
    print(n,"is a leap year.")
  elif(n%4==0)and(n%100!=0):
    print(n,"is a leap year.")
  else:
    print(n,"is not a leap year.")
else:
    print("Enter a valid year!")
